{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "aws/aws-sdk-php": "~2.4",
        "benargo/laravel-json-schema": "dev-master",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "~4.0",
        "graham-campbell/markdown": "^10.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-notification-channels/discord": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "logansua/blizzard-api-client": "^2.0",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "marquine/eloquent-uuid": "^1.0",
        "predis/predis": "^1.1",
        "restcord/restcord": "^0.2.2",
        "socialiteproviders/battlenet": "dev-master",
        "socialiteproviders/discord": "^2.0",
        "socialiteproviders/patreon": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^5.11",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
